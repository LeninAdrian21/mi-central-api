{
  "routes": [
    {
      "method": "GET",
      "path": "/abonos",
      "handler": "abonos.find",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }

    },
    {
      "method": "GET",
      "path": "/abonos/pagination",
      "handler": "abonos.findAbonos",
      "config": {
        "policies": []
      }

    },
    {
      "method": "GET",
      "path": "/abonos/buscar",
      "handler": "abonos.buscarAbonos"
    },
    {
      "method": "GET",
      "path": "/abonos/count",
      "handler": "abonos.count",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "GET",
      "path": "/abonos/:id",
      "handler": "abonos.findOne",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "POST",
      "path": "/abonos",
      "handler": "abonos.create",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "PUT",
      "path": "/abonos/:id",
      "handler": "abonos.update",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "DELETE",
      "path": "/abonos/:id",
      "handler": "abonos.delete",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    }
  ]
}
