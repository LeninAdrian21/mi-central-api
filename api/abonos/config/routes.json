{
  "routes": [
    {
      "method": "GET",
      "path": "/abonos",
      "handler": "abonos.find",
      "config": {
        "policies": ["global::AuthenticationPermissions"]
      }
    },
    {
      "method": "GET",
      "path": "/abonos/count",
      "handler": "abonos.count",
      "config": {
        "policies": ["global::AuthenticationPermissions"]
      }
    },
    {
      "method": "GET",
      "path": "/abonos/:id",
      "handler": "abonos.findOne",
      "config": {
        "policies": ["global::AuthenticationPermissions"]
      }
    },
    {
      "method": "POST",
      "path": "/abonos",
      "handler": "abonos.create",
      "config": {
        "policies": ["global::AuthenticationPermissions"]
      }
    },
    {
      "method": "PUT",
      "path": "/abonos/:id",
      "handler": "abonos.update",
      "config": {
        "policies": ["global::AuthenticationPermissions"]
      }
    },
    {
      "method": "DELETE",
      "path": "/abonos/:id",
      "handler": "abonos.delete",
      "config": {
        "policies":["global::AuthenticationPermissions"]
      }
    },
    {
      "method": "POST",
      "path": "/abonos/table/files",
      "handler": "abonos.file",
      "config": {
        "policies": []
      }
    }
  ]
}
