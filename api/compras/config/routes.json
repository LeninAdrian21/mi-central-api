{
  "routes": [
    {
      "method": "GET",
      "path": "/compras",
      "handler": "compras.find",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "GET",
      "path": "/compras/count",
      "handler": "compras.count",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "GET",
      "path": "/compras/:id",
      "handler": "compras.findOne",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "POST",
      "path": "/compras",
      "handler": "compras.create",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "PUT",
      "path": "/compras/:id",
      "handler": "compras.update",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "DELETE",
      "path": "/compras/:id",
      "handler": "compras.delete",
      "config": {
        "policies": ["global::permissions"]
      }
    }
  ]
}
