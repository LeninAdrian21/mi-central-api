{
  "routes": [
    {
      "method": "GET",
      "path": "/creditos",
      "handler": "credito.find",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "GET",
      "path": "/creditos/count",
      "handler": "credito.count",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "GET",
      "path": "/creditos/:id",
      "handler": "credito.findOne",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "POST",
      "path": "/creditos",
      "handler": "credito.create",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "PUT",
      "path": "/creditos/:id",
      "handler": "credito.update",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "DELETE",
      "path": "/creditos/:id",
      "handler": "credito.delete",
      "config": {
        "policies": ["global::permissions"]
      }
    }
  ]
}
