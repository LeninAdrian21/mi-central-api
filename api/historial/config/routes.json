{
  "routes": [
    {
      "method": "GET",
      "path": "/historials",
      "handler": "historial.find",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "GET",
      "path": "/historials/count",
      "handler": "historial.count",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "GET",
      "path": "/historials/:id",
      "handler": "historial.findOne",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "POST",
      "path": "/historials",
      "handler": "historial.create",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "PUT",
      "path": "/historials/:id",
      "handler": "historial.update",
      "config": {
        "policies": ["global::permissions"]
      }
    },
    {
      "method": "DELETE",
      "path": "/historials/:id",
      "handler": "historial.delete",
      "config": {
        "policies": ["global::permissions"]
      }
    }
  ]
}
